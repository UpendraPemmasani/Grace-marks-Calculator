<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
 
<body>
<style>
 

 

 
 
body{
    background-image: linear-gradient(to top, #fbc2eb 0%, #a6c1ee 100%);
  height: 99.5vh;
   
    justify-content: center;
    align-items: center;
     text-align: center;
}
 
.home {
 
bottom: 10px;
right: 10px;
 
font-size:15px;
  border-radius: 3px;
  background-color: #f4511e;
  border: none;
  color: #FFFFFF;
  text-align: center;
  font-size: 15px;
  padding: 7px;
  width: 70px;
  transition: all 0.5s;
  cursor: pointer;
  margin: 5px;
}
  
 
  
 
h2{
  text-align: center;
}


</style>
 
<table id="table" >  
<thread>  
<tr>    
<form method="GET", action="http://localhost:8000/">     
</tr>  

 

  <h2>
    Grading Scheme
  </h2>

</thead>  
<tbody>
    <div class="container">
     
        <table class="table table-striped">
          <thead>
            <tr> 
                <th>Course Code</th>
                <th>O</th>
                <th>A+</th>
                <th>A</th> 
                <th>B+</th>
                <th>B</th>
                <th>C</th>
                <th>P</th>
                <th>F</th>
              </tr>
            </thead>
    
         

            <div class="container"> 
        <table class="table table-striped">
        <% grading.forEach(function (grade) { %>
            <tr> 
                <td ><%=  grade.coursecode%>    </td> 
                <td><%=  grade.O%>  </td> 
                <td> <%= grade.A%>  </td>
                <td> <%= grade.A%>      </td> 
              
                <td> <%=  grade.B%>    </td>
                <td>    <%=  grade.B%> </td>   
                <td>  <%= grade.C%> </td>  
                <td>  <%= grade.P%> </td>
                <td>  <%= grade.F%> </td>  
              </tr> 
     
                
        <% }) %>
 

  
</tbody>
</table>


 

 
</div>
    
</body>

 
 
<button class="home"  onclick="window.close()">Close</button>

</html>
